{
  "name": "skeleton-framework",
  "version": "1.2.5",
  "license": "MIT",
  "homepage": "https://github.com/NeuraLabs/skeleton-framework#readme",
  "description": "Neura adaptation of skeleton framework",
  "author": "Neura Skeleton Framework",
  "contributors": [
    "Matias Crivolotti <matias@theneura.com>"
  ],
  "main": "index.js",
  "scripts": {
    "start": "npm run watch & npm run serve",
    "build": "node index",
    "watch": "npm-scripts-watcher",
    "serve": "live-server --port=3000 --mount=/:/dist --open=/dist --watch=/dist"
  },
  "watch": {
    "src/**/*.css": [
      "build"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NeuraLabs/skeleton-framework"
  },
  "bugs": {
    "url": "https://github.com/NeuraLabs/skeleton-framework"
  },
  "keywords": [
    "css",
    "framework",
    "skeleton",
    "skeleton-framework"
  ],
  "devDependencies": {
    "live-server": "^1.1.0",
    "npm-scripts-watcher": "^1.0.2",
    "skeleton-postcss": "^1.0.4"
  }
}
